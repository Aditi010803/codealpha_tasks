    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        int check=0;
        email=jTextField1.getText();
        if (email.equals(""))
        {
            check=1;
            JOptionPane.showMessageDialog(null, "Email Field is Required");
        }
        else
        {
            ResultSet rs=select.getData("select *from user where email='"+email+"'");
            try{
                if(rs.next()){
                    check=1;
                    jTextField2.setEditable(false);
                    jTextField1.setEditable(false);
                    jTextField2.setText(rs.getString(4));
                }
            }
            catch(Exception e)
            {
               JOptionPane.showMessageDialog(null, e);  
            }
        }
        if(check==0)
           JOptionPane.showMessageDialog(null, "Incorrect Email"); 
        
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        int check=0;
        String secq=jTextField2.getText();
        String answer=jTextField3.getText();
        String newpass=jPasswordField1.getText();
        if(answer.equals("")||newpass.equals(""))
        {
            check=1;
            JOptionPane.showMessageDialog(null, "All Field is Required"); 
        }
        else
        {
            ResultSet rs=select.getData("select *from user where email='"+email+"'and secq='"+secq+"' and answer='"+answer+"'");
            try{
                if(rs.next()){
                    check=1;
                    insertupdatedata.setData("update user set pass='"+newpass+"'where email='"+email+"'","Password set Successfully");
                    setVisible(false);
                      new forgotpass().setVisible(true);
                }
                
            }
            catch(Exception e){
                 JOptionPane.showMessageDialog(null, e); 
            }
        }
        if(check==0)
        JOptionPane.showMessageDialog(null, "Incorrect Answer"); 
        
    }                                        

    private void jPasswordField1ActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // TODO add your handling code here:
    }                                               

